<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="@Url.Content("~/Content/2-col-portfolio.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/4-col-portfolio.css")" rel="stylesheet" type="text/css" />

   
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @{
        Html.RenderPartial("_HeaderNavBarPartial");
    }
        @RenderBody()
        
    @{
        Html.RenderPartial("_FooterPartial");
    }
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
